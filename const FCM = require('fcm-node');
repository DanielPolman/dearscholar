const FCM = require('fcm-node');
// Replace these with your own values.
const apiKey = 'AAAAYbKxUSw:APA91bEc8IPLORC5d-k6drQR-YJ5ntLw2ZbLsvT9U-pmO0EHvbIAqwW33w_tPN1Zz1cUdIFsllSNfs2kRq2sfERZNhtX7rOh4_MIlMQxrZtYSxwvjennKVPL9eybiotWmCrqWnKQgi08';
const deviceID = 'ffbvtBMuUFKg:APA91bFiNBZ-TxyrJA-BO4oP9A6AgbmMOyALu6WPmGFSxqJuGrBO7BRzkwdKpq8zn167lG8djpZPAWphRAcJaog7U9RwlcfjEqvbdjpZWe8KWrhy4CkJfQ7n5AreCpJRNmH0T69r6PuL';
const fcm = new FCM(apiKey);

const message = {
  to: deviceID,
  data: {
    title: 'Force Start',
    message: 'This notification should restart the app',
    "force-start": "1"
  }
};

fcm.send(message, (err, response) => {
  if (err) {
    console.log(err);
    console.log('Something has gone wrong!');
  } else {
    console.log('Successfully sent with response: ', response);
  }
});